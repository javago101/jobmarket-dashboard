{
  "name": "jobmarket-dashboard",
  "version": "1.0.0",
  "description": "Job Market Dashboard Monorepo",
  "scripts": {
    "start": "concurrently \"pnpm run server\" \"pnpm run client\"",
    "dev": "concurrently \"pnpm run server:dev\" \"pnpm run client:dev\"",
    "server": "cd server && pnpm run start",
    "server:dev": "cd server && pnpm run dev",
    "client": "cd client && pnpm run start",
    "client:dev": "cd client && pnpm run dev",
    "install:all": "pnpm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}